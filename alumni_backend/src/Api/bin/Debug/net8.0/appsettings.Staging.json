{
  "Database": {
    "UsePostgreSQL": true
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=staging-host;Port=5432;Database=staging_alumni_db;Username=staging_user;Password=staging_password"
  },
  "Authentication": {
    "Jwt": {
      "Key": "alumni-super-secret-key-32-characters-minimum-required-staging",
      "Issuer": "alumni-api-staging",
      "Audience": "alumni-users-staging",
      "ExpirationMinutes": 60
    }
  },
  "AWS": {
    "S3": {
      "BucketName": "alumni-app-images-staging",
      "Region": "us-east-1",
      "AccessKey": "staging-access-key",
      "SecretKey": "staging-secret-key"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}